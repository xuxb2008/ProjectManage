<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="DomainDLL.DictItem, DomainDLL" table="DictItem">
		<id name="ID" type="String" unsaved-value="null">
			<column name="ID" sql-type="varchar" not-null="true"/>
			<generator class="assigned" />
		</id>
		<property name="DictNo" type="String">
			<column name="DictNo" sql-type="varchar" not-null="true"/>
		</property>
    <property name="No" type="String">
      <column name="No" sql-type="varchar" not-null="true"/>
    </property>
		<property name="Name" type="String">
			<column name="Name" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="Remark" type="String">
			<column name="Remark" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="CREATED" type="DateTime">
			<column name="CREATED" sql-type="datetime" not-null="false"/>
		</property>
		<property name="UPDATED" type="DateTime">
			<column name="UPDATED" sql-type="datetime" not-null="false"/>
		</property>
	</class>
</hibernate-mapping>