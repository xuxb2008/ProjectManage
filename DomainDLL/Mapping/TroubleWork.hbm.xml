    <property name="ActualWorkload" type="decimal">
      <column name="ActualWorkload" sql-type="decimal" not-null="true"/>
    </property>
    <property name="Manager" type="String">
      <column name="Manager" sql-type="varchar" not-null="false"/>
    </property>
